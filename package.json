{
  "name": "safelogins",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start2": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start": "npm run start:client && npm run start:server",
    "start:client": "cd ./client && npm run start",
    "start:server": "cd ./server && npm run start",
    "client:deploy": "cd client && npm run deploy",
    "server:deploy": "cd server && npm run deploy",
    "install:client": "cd client && npm i",
    "install:server": "cd server && npm i",
    "install": "npm run install:server && npm run install:client",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build",
    "build": "npm run build:client && npm run build:server",
    "install:build": "npm run install && npm run build",
    "deploy": "npm run install:build && npm run client:deploy & npm run server:deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewaclark248/SafeLogins.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewaclark248/SafeLogins/issues"
  },
  "homepage": "https://github.com/andrewaclark248/SafeLogins#readme",
  "dependencies": {
    "concurrently": "^8.2.0",
    "typescript": "^5.1.6"
  }
}
